#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int main()
{int n,factorial=1,i=1; 
cout<<"Valor:";cin>>n; //5
while(i<=n)
     {factorial=factorial*i; 
      i=i+1; //1<5 sigue a 2<5 sigue a 3<5 sigue a 4<5 sigue 5=5 trmina(se va almacenando 1*2*3*4*5)
     }
cout<<factorial;

	getch();
	return 0;
}
